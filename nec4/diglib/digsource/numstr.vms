	SUBROUTINE NUMSTR(JVAL,BSTRNG)
	BYTE BSTRNG(2)
C
C	THIS SUBROUTINE CONVERTS "IVAL" TO A STRING WITH NO LEADING
C	 OR TRAILING SPACES.
C
	BYTE NUMBER(10), BNULL
	DATA BNULL /0/
C
	ENCODE (9,11,NUMBER) JVAL
11	FORMAT(I9)
	NUMBER(10) = BNULL
	CALL STRPBL(NUMBER)
	CALL SCOPY(NUMBER,BSTRNG)
	RETURN
	END
