	PROGRAM TEST
C
C	This program read a .FNT file and displays the selected character
C	to provide a quick look at the characters in the font.   This thing
C	is completely unsupported and is best deleted.
C
	INTEGER BX(128), BY(128)
	BYTE FILNAM(80)
	LOGICAL LMOVE
C
	CALL SELDEV(4)
C
	CALL SREPLY('Enter font file',0,79,FILNAM,LL)
	OPEN (UNIT=1,NAME=FILNAM,TYPE='OLD',READONLY)
C
1	CONTINUE
	ICHAR = IREPLY('Enter character number of plot',65,33,127,.TRUE.)
	REWIND 1

10		CONTINUE
		READ (1,11) JCHAR,NSTROKES,ILEFT,IRIGHT,(BX(I),BY(I), I=1,7)
11		FORMAT(18I4)
		IF (JCHAR .EQ. ICHAR) GO TO 100
		NSTROKES = NSTROKES - 16
		IF (NSTROKES .LE. 0) GO TO 10
20			CONTINUE
			READ (1,11)
			NSTROKES = NSTROKES - 18
			IF (NSTROKES .LE. 0) GO TO 10
			GO TO 20
100	CONTINUE
	CALL BGNPLT
	NSTROKES = (NSTROKES-4)/2	!STROKES COUNTS WIDTH PAIR AND END
D	TYPE *,'NSTROKES IS ',NSTROKES
	MAXI = 7
	I = 1
	LMOVE = .TRUE.
	SCALE = 0.25
	XOFF = 8.0
	YOFF = 2.0
110		CONTINUE
		IF (I .GT. MAXI) THEN
			READ (1,11) (BX(I),BY(I), I=1,9)
			I = 1
			MAXI = 9
		ENDIF
D		TYPE *,'COORDINATE IS ',BX(I),BY(I)
		IF (BX(I) .EQ. -999) THEN
			LMOVE = .TRUE.
			GO TO 130
		ENDIF
		X = SCALE*(BX(I)-ILEFT) + XOFF
		Y = SCALE*(BY(I)+20) + YOFF
		IF (LMOVE) THEN
			CALL GSMOVE(X,Y)
		    ELSE
			CALL GSDRAW(X,Y)
		ENDIF
		LMOVE = .FALSE.
130		CONTINUE
		I = I + 1
		NSTROKES = NSTROKES-1
		IF (NSTROKES .GT. 0) GO TO 110
	CALL GSMOVE(0.0,SCALE*11.0+YOFF)
	CALL GSDRAW(GSXLCM(),SCALE*11.0+YOFF)
	CALL ENDPLT
	GO TO 1
	END
